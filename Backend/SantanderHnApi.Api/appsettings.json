{
  "HackerNews": {
    "BaseUrl": "https://hacker-news.firebaseio.com/",
    "BestStoriesCacheSeconds": 120,
    "ItemCacheSeconds": 600,
    "MaxConcurrency": 10,
    "MaxItems": 500,
    "HttpTimeoutSeconds": 5,
    "MaxRetries": 3,
    "RetryBaseDelaySeconds": 0.5,
    "RetryJitterMaxMilliseconds": 250,
    "CircuitBreakerFailures": 5,
    "CircuitBreakerBreakSeconds": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
